{
  "name": "eslint-plugin-untitled",
  "version": "0.0.1",
  "description": "TODO",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "untitled"
  ],
  "license": "UNLICENSED",
  "private": true,
  "repository": "billyzkid/untitled",
  "bugs": {
    "url": "https://github.com/billyzkid/untitled/issues"
  },
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=6"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "*",
    "babel-core": "*",
    "babel-jest": "*",
    "babel-preset-latest": "*",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-react": "*",
    "jest-cli": "*",
    "nodemon": "^1.11.0",
    "rimraf": "*"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "scripts": {
    "build": "babel src --out-dir build --ignore test.js",
    "build:w": "npm run build -- --watch",
    "clean": "rimraf build",
    "lint": "eslint src",
    "lint:w": "nodemon --watch src --exec npm run lint",
    "prebuild": "npm run clean",
    "pretest": "npm run lint",
    "test": "jest",
    "test:w": "npm run test -- --watch"
  },
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "node": true,
      "jest": true
    },
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ],
      "eol-last": [
        "error",
        "never"
      ],
      "quotes": [
        "error",
        "double"
      ]
    }
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "src/.*\\.test\\.js$",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}
