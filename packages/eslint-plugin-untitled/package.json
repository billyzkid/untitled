{
  "name": "eslint-plugin-untitled",
  "version": "0.0.1",
  "description": "TODO",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "untitled"
  ],
  "author": "Will Allan <billyzkid@yahoo.com> (https://github.com/billyzkid)",
  "homepage": "https://billyzkid.github.io/untitled/",
  "bugs": "https://github.com/billyzkid/untitled/issues",
  "repository": "billyzkid/untitled",
  "license": "MIT",
  "private": true,
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-latest": "^6.22.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "jest-cli": "^18.1.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.5.4"
  },
  "peerDependencies": {
    "eslint": "^3.15.0"
  },
  "scripts": {
    "build": "babel src --out-dir build --ignore test.js",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf build",
    "lint": "eslint src",
    "lint:watch": "nodemon --watch src --exec npm run lint",
    "prebuild": "npm run clean",
    "pretest": "npm run lint",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "node": true,
      "jest": true
    },
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ],
      "eol-last": [
        "error",
        "never"
      ],
      "quotes": [
        "error",
        "double"
      ]
    }
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "src/.*\\.test\\.js$",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}