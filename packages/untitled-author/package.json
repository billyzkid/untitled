{
  "name": "untitled-author",
  "version": "0.0.1",
  "description": "TODO",
  "keywords": [
    "untitled",
    "author"
  ],
  "author": "Will Allan <billyzkid@yahoo.com> (https://github.com/billyzkid)",
  "homepage": "https://billyzkid.github.io/untitled/",
  "bugs": "https://github.com/billyzkid/untitled/issues",
  "repository": "billyzkid/untitled",
  "license": "MIT",
  "private": true,
  "preferGlobal": true,
  "main": "build/index.js",
  "bin": {
    "untitled-author": "build/index.js"
  },
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-preset-untitled": "^0.0.1",
    "eslint": "^3.15.0",
    "eslint-plugin-untitled": "^0.0.1",
    "jest-cli": "^18.1.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.5.4"
  },
  "scripts": {
    "build": "babel src --out-dir build --ignore test.js",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf build",
    "lint": "eslint src",
    "lint:watch": "nodemon --watch src --exec npm run lint",
    "prebuild": "npm run clean",
    "pretest": "npm run lint",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "babel": {
    "presets": [
      "untitled"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:untitled/recommended"
    ],
    "env": {
      "node": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "untitled"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "src/.*\\.test\\.js$",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}